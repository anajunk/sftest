<UserControl
	x:Class="New_SF_IT.Volatality_Designs.Intraday_TopBottom" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:local="clr-namespace:New_SF_IT.Volatality_Designs">
	<ScrollViewer
		VerticalScrollBarVisibility="Auto">
		<Grid>
			<Grid.RowDefinitions>
				<RowDefinition
					Height="7.5*" />
			</Grid.RowDefinitions>
			<Grid
				VerticalAlignment="Top"
				Row="1"
				Margin="50">
				<FrameworkElement.Resources>
					<ResourceDictionary>
						<Style
							TargetType="{x:Type Paragraph}"
							x:Key="{x:Type Paragraph}">
							<Setter
								Property="FontSize"
								Value="14" />
							<Setter
								Property="FontWeight"
								Value="Normal" />
						</Style>
					</ResourceDictionary>
				</FrameworkElement.Resources>
				<FlowDocumentScrollViewer
					Grid.Column="0"
					HorizontalScrollBarVisibility="Hidden"
					VerticalScrollBarVisibility="Hidden">
					<FlowDocument
						Name="GAV_buyTargets">
						<Table
							BorderBrush="#FFFFFFFF"
							BorderThickness="1">
							<FrameworkContentElement.Resources>
								<ResourceDictionary>
									<Style
										TargetType="{x:Type TableCell}"
										x:Key="{x:Type TableCell}">
										<Setter
											Property="TableCell.Padding"
											Value="0" />
										<Setter
											Property="TableCell.LineHeight"
											Value="20" />
										<Setter
											Property="FontSize"
											Value="15" />
										<Setter
											Property="FontFamily"
											Value="Tunga" />
										<Setter
											Property="Background"
											Value="#F7F9FC" />
										<Setter
											Property="Foreground"
											Value="#FF000000" />
										<Setter
											Property="FontWeight"
											Value="SemiBold" />
										<Setter
											Property="TableCell.TextAlignment"
											Value="Center" />
										<Setter
											Property="TableCell.BorderBrush"
											Value="#BABABA" />
										<Setter
											Property="TableCell.BorderThickness"
											Value="0.5" />
									</Style>
									<Style
										TargetType="{x:Type Paragraph}"
										x:Key="{x:Type Paragraph}">
										<Setter
											Property="FontWeight"
											Value="SemiBold" />
										<Setter
											Property="Block.Padding"
											Value="70 0 0 0" />
										<Setter
											Property="Block.TextAlignment"
											Value="Left" />
									</Style>
								</ResourceDictionary>
							</FrameworkContentElement.Resources>
							<TableRowGroup>
								<TableRow>
									<TableCell>
										<Paragraph
											Name="symbolTop"
											FontWeight="Bold"
											Background="#C5EBAA"
											Foreground="#FF000000" />
									</TableCell>
									<TableCell>
										<Paragraph
											FontWeight="Bold"
											Background="#C5EBAA"
											Foreground="#49243E">Intraday Top</Paragraph>
									</TableCell>
									<TableCell>
										<Paragraph
											Name="symbolBot"
											FontWeight="Bold"
											Background="#FFB5B7"
											Foreground="#FF000000" />
									</TableCell>
									<TableCell>
										<Paragraph
											FontWeight="Bold"
											Background="#FFB5B7"
											Foreground="#49243E">Intraday Bottom</Paragraph>
									</TableCell>
								</TableRow>
								<TableRow>
									<TableCell>
										<Paragraph
											Name="display_GAV_buy_C1_Name">1st Top</Paragraph>
									</TableCell>
									<TableCell>
										<Paragraph
											Name="Top1stC" />
									</TableCell>
									<TableCell>
										<Paragraph
											Name="display_GAV_buy_C1_Name1">1st Bottom</Paragraph>
									</TableCell>
									<TableCell>
										<Paragraph
											Name="Bottom1stC" />
									</TableCell>
								</TableRow>
								<TableRow>
									<TableCell>
										<Paragraph>3rd Top</Paragraph>
									</TableCell>
									<TableCell>
										<Paragraph
											Name="Top3rdC" />
									</TableCell>
									<TableCell>
										<Paragraph>3rd Bottom</Paragraph>
									</TableCell>
									<TableCell>
										<Paragraph
											Name="Bottom3rdC" />
									</TableCell>
								</TableRow>
								<TableRow>
									<TableCell>
										<Paragraph>5th Top</Paragraph>
									</TableCell>
									<TableCell>
										<Paragraph
											Name="Top5thC" />
									</TableCell>
									<TableCell>
										<Paragraph>5th Bottom</Paragraph>
									</TableCell>
									<TableCell>
										<Paragraph
											Name="Bottom5thC" />
									</TableCell>
								</TableRow>
								<TableRow>
									<TableCell>
										<Paragraph>7th top</Paragraph>
									</TableCell>
									<TableCell>
										<Paragraph
											Name="Top7thC" />
									</TableCell>
									<TableCell>
										<Paragraph>7th Bottom</Paragraph>
									</TableCell>
									<TableCell>
										<Paragraph
											Name="Bottom7thC" />
									</TableCell>
								</TableRow>
								<TableRow>
									<TableCell>
										<Paragraph>9th Top</Paragraph>
									</TableCell>
									<TableCell>
										<Paragraph
											Name="Top9thC" />
									</TableCell>
									<TableCell>
										<Paragraph>9th Bottom</Paragraph>
									</TableCell>
									<TableCell>
										<Paragraph
											Name="Bottom9thC" />
									</TableCell>
								</TableRow>
								<TableRow>
									<TableCell>
										<Paragraph>11th Top</Paragraph>
									</TableCell>
									<TableCell>
										<Paragraph
											Name="Top11thC" />
									</TableCell>
									<TableCell>
										<Paragraph>11th Bottom</Paragraph>
									</TableCell>
									<TableCell>
										<Paragraph
											Name="Bottom11thC" />
									</TableCell>
								</TableRow>
								<TableRow>
									<TableCell>
										<Paragraph>13th Top</Paragraph>
									</TableCell>
									<TableCell>
										<Paragraph
											Name="Top13thC" />
									</TableCell>
									<TableCell>
										<Paragraph>13th Bottom</Paragraph>
									</TableCell>
									<TableCell>
										<Paragraph
											Name="Bottom13thC" />
									</TableCell>
								</TableRow>
								<TableRow>
									<TableCell>
										<Paragraph>15th Top</Paragraph>
									</TableCell>
									<TableCell>
										<Paragraph
											Name="Top15thC" />
									</TableCell>
									<TableCell>
										<Paragraph>15th Bottom</Paragraph>
									</TableCell>
									<TableCell>
										<Paragraph
											Name="Bottom15thC" />
									</TableCell>
								</TableRow>
								<TableRow>
									<TableCell>
										<Paragraph>17th Top</Paragraph>
									</TableCell>
									<TableCell>
										<Paragraph
											Name="Top17thC" />
									</TableCell>
									<TableCell>
										<Paragraph>17th Bottom</Paragraph>
									</TableCell>
									<TableCell>
										<Paragraph
											Name="Bottom17thC" />
									</TableCell>
								</TableRow>
							</TableRowGroup>
						</Table>
					</FlowDocument>
				</FlowDocumentScrollViewer>
			</Grid>
		</Grid>
	</ScrollViewer>
</UserControl>
